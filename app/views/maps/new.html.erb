<%= flash[:notice] %>

<%= form_for @map do |f| %>
  <div class="form-group">
    <h4><%= f.label :point_collection %></h4>
    <%= f.file_field :point_collection %>
    <%= f.text_field :name %>
    <%= f.text_field :description %>
  </div>
  <div class="form-group">
    <%= f.submit "Upload", class: 'btn btn-success' %>
  </div>
<% end %>
